<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
    <PropertyGroup>
        <Version>0.11.0</Version>
        <Authors>clown;cube-soft</Authors>
        <Company>CubeSoft</Company>
        <Copyright>Copyright © 2010 CubeSoft, Inc.</Copyright>
        <StartupObject>Cube.FileSystem.SevenZip.Ice.Tests.Program</StartupObject>
        <OutputType>Exe</OutputType>
        <TargetFramework>net45</TargetFramework>
        <UseWindowsForms>true</UseWindowsForms>
        <LangVersion>latest</LangVersion>
        <IsPackable>false</IsPackable>
        <EnableDefaultNoneItems>false</EnableDefaultNoneItems>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <DefineConstants Condition=" '$(Configuration)' == 'Debug' ">DEBUG;TRACE</DefineConstants>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Platform)' == 'AnyCPU' ">
        <OutputPath>bin\Any CPU\$(Configuration)\</OutputPath>
        <DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\Applications\Ice\Main\Cube.FileSystem.SevenZip.Ice.Main.csproj" />
        <ProjectReference Include="..\..\Applications\Ice\Settings\Cube.FileSystem.SevenZip.Ice.Settings.csproj" />
        <PackageReference Include="Cube.Tests" Version="5.0.*" />
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.11.*" />
        <PackageReference Include="NUnit3TestAdapter" Version="4.0.*" />
        <PackageReference Include="Appveyor.TestLogger" Version="2.0.*" />
    </ItemGroup>
    <ItemGroup>
        <None Include="App.config" />
        <None Include="Examples\**\*" CopyToOutputDirectory="Always" />
    </ItemGroup>
</Project>